<?xml version="10"?>
<data >
    <class name="Advanced activations" >
        <layer name="LeakyReLU">
            <param name="alpha" start="0.1" end="0.7" step_size="0.05"/>
        </layer>

        <layer name="PReLU"/>
        <layer name="ELU"/>
        <layer name="ThresholdedReLU"/>
        <layer name="Softmax"/>
    </class>

    <class name="Convolution layers">
        
        <layer name="Conv1D"  />
        <layer name="Conv2D">
            <param name="filters" start="1" end="30" step_size="1"/>
        </layer>
            <layer name="Conv3D"/>
            <layer name="Conv1DTranspose"/>
            <layer name="Conv2DTranspose"/>
            <layer name="Conv3DTranspose"/>
            <layer name="SeparableConv1D"/>
            <layer name="SeparableConv2D"/>
            <layer name="DepthwiseConv2D"/>
        </class>

        <class name="Image processing layers">
            <layer name="UpSampling1D"/>
            <layer name="UpSampling2D"/>
            <layer name="UpSampling3D"/>
            <layer name="ZeroPadding1D"/>
            <layer name="ZeroPadding2D"/>
            <layer name="ZeroPadding3D"/>
            <layer name="Cropping1D"/>
            <layer name="Cropping2D"/>
            <layer name="Cropping3D"/>
        </class>

        <class name="Core layers">
            <layer name="Masking"/>
            <layer name="Dropout"/>
            <layer name="SpatialDropout1D"/>
            <layer name="SpatialDropout2D"/>
            <layer name="SpatialDropout3D"/>
            <layer name="Activation"/>
            <layer name="Reshape"/>
            <layer name="Permute"/>
            <layer name="Flatten"/>
            <layer name="RepeatVector"/>
            <layer name="Lambda"/>
            <layer name="Dense"/>
            <layer name="ActivityRegularization"/>
        </class>

        <class name="Dense Attention layers">
            <layer name="AdditiveAttention"/>
            <layer name="Attention"/>
        </class>

        <class name="Embedding layers">
            <layer name="Embedding"/>
        </class>

        <class name="Einsum-based dense layer">
            <layer name="EinsumDense"/>
        </class>

        <class name="Multi-head Attention layer">
            <layer name="MultiHeadAttention"/>
        </class>

        <class name="Locally-connected layers">
            <layer name="LocallyConnected1D"/>
            <layer name="LocallyConnected2D"/>
        </class>

        <class name="Merge layers">
            <layer name="Add"/>
            <layer name="Subtract"/>
            <layer name="Multiply"/>
            <layer name="Average"/>
            <layer name="Maximum"/>
            <layer name="Minimum"/>
            <layer name="Concatenate"/>
            <layer name="Dot"/>
            <layer name="add"/>
            <layer name="subtract"/>
            <layer name="multiply"/>
            <layer name="average"/>
            <layer name="maximum"/>
            <layer name="minimum"/>
            <layer name="concatenate"/>
            <layer name="dot"/>
        </class>

        <class name="Noise layers">
            <layer name="AlphaDropout"/>
            <layer name="GaussianNoise"/>
            <layer name="GaussianDropout"/>
        </class>

        <class name="Normalization layers">
            <layer name="LayerNormalization"/>
            <layer name="SyncBatchNormalization"/>
            <layer name="BatchNormalization"/>
        </class>

        <class name="Kernelized layers">
            <layer name="RandomFourierFeatures"/>
        </class>

        <class name="Pooling layers">
            <layer name="MaxPooling1D"/>
            <layer name="MaxPooling2D"/>
            <layer name="MaxPooling3D"/>
            <layer name="AveragePooling1D"/>
            <layer name="AveragePooling2D"/>
            <layer name="AveragePooling3D"/>
            <layer name="GlobalAveragePooling1D"/>
            <layer name="GlobalAveragePooling2D"/>
            <layer name="GlobalAveragePooling3D"/>
            <layer name="GlobalMaxPooling1D"/>
            <layer name="GlobalMaxPooling2D"/>
            <layer name="GlobalMaxPooling3D"/>
        </class>

        <class name="Recurrent layers">
            <layer name="RNN"/>
            <layer name="AbstractRNNCell"/>
            <layer name="StackedRNNCells"/>
            <layer name="SimpleRNNCell"/>
            <layer name="PeepholeLSTMCell"/>
            <layer name="SimpleRNN"/>
            <layer name="GRU"/>
            <layer name="GRUCell"/>
            <layer name="LSTM"/>
            <layer name="LSTMCell"/>
        </class>

        <class name="Convolutional-recurrent layers">
            <layer name="ConvLSTM2D"/>
        </class>

        <class name="CuDNN recurrent layers">
            <layer name="CuDNNLSTM"/>
            <layer name="CuDNNGRU"/>
        </class>

    </data>
