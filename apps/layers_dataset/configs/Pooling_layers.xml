            </layer>
            <layer name="GlobalAveragePooling3D">
            </layer>
            <layer name="GlobalMaxPooling1D">
            </layer>
            <layer name="GlobalMaxPooling2D">
            </layer>
            <layer name="GlobalMaxPooling3D">
            </layer> -->
    </class>
</data>